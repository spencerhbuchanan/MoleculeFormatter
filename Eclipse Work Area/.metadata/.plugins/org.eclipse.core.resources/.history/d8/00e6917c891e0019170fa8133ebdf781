public class Main
{

	public static void main(String[] args)
	{
		System.out.println("Hello World Test");
		
		Clock digitalClock = new Clock();
		
		digitalClock.initializeClock();
		
		System.out.println(digitalClock.getSeconds());
		System.out.println(digitalClock.getMinutes());
		System.out.println(digitalClock.getHours());
		
		while(TRUE)
		{
			digitalClock.printTime();
		}
		
	}

}
